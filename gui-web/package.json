{
    "name": "@archiveboy/gui-web",
    "scripts": {
        "build": "npm run configure-target-env-electron-renderer && gulp build --color",
        "configure-target-env-electron-renderer": "gulp configure-target-env-electron-renderer --color",
        "configure-target-env-web": "gulp configure-target-env-web --color",
        "dev": "npm run configure-target-env-web && ng serve --configuration=web",
        "dev-electron-renderer": "npm run configure-target-env-electron-renderer && ng serve",
        "test": "jest --color",
        "lint": "gulp lint --color",
        "uplate": "ncu -u"
    },
    "jest": {
        "preset": "jest-preset-angular",
        "moduleNameMapper": {
            "@archiveboy/gui-web/(.*)": "<rootDir>/src/main//$1"
        },
        "setupFilesAfterEnv": [
            "<rootDir>/jest.config.ts"
        ]
    },
    "dependencies": {
        "@angular/animations": "~10.1.3",
        "@angular/cdk": "^10.2.2",
        "@angular/common": "~10.1.3",
        "@angular/compiler": "~10.1.3",
        "@angular/core": "~10.1.3",
        "@angular/flex-layout": "^10.0.0-beta.32",
        "@angular/forms": "~10.1.3",
        "@angular/material": "^10.2.2",
        "@angular/platform-browser": "~10.1.3",
        "@angular/platform-browser-dynamic": "~10.1.3",
        "@angular/router": "~10.1.3",
        "@archiveboy/api": "^0.0.1",
        "@archiveboy/api-local": "^0.0.1",
        "@ngrx/effects": "^10.0.0",
        "@ngrx/store": "^10.0.0",
        "@ngrx/store-devtools": "^10.0.0",
        "rxjs": "~6.6.3",
        "tslib": "^2.0.1",
        "zone.js": "~0.11.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1001.3",
        "@angular/cli": "~10.1.3",
        "@angular/compiler-cli": "~10.1.3",
        "@angular/language-service": "~10.1.3",
        "@archiveboy/toolbox": "^0.0.1",
        "@types/jest": "^26.0.14",
        "@types/node": "^12.12.6",
        "codelyzer": "^6.0.0",
        "electron": "^10.1.2",
        "jest": "^26.4.2",
        "jest-preset-angular": "^8.3.1"
    }
}
